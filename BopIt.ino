const unsigned char background_tone[] PROGMEM = {400, 300, 250, 200, -1, 300, 350};
const unsigned char background_duration[] PROGMEM = {200, 200, 150, 250, 100, 200, 250};

const unsigned char defeat_tone[] PROGMEM = {200, 175, 150, 125};
const unsigned char defeat_duration[] PROGMEM = {300, 300, 300, 300};

const unsigned char victory_tone[] PROGMEM = {300, 225, 350, 375, -1, 375, -1, 375};
const unsigned char victory_duration[] PROGMEM = {150, 150, 150, 100, 50, 100, 50, 300};


const unsigned char success_tone[] PROGMEM = {300, 350, 400, 450};
const unsigned char success_duration[] PROGMEM = {150, 150, 150, 150};

void setup() {

  // SSD1306_SWITCHCAPVCC = generate display voltage from 3.3V internally
  if(!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    for(;;); // Don't proceed, loop forever
  }
 
  display.setTextSize(1); //Setup text size
  display.setTextColor(WHITE); //Setup font color 

  //Blue Button
  pinMode(1, OUTPUT);
  pinMode(6, INPUT_PULLUP);

  //Yellow Button
  pinMode(A0, OUTPUT);
  pinMode(5, INPUT_PULLUP);

  //Red Button
  pinMode(A1, OUTPUT);
  pinMode(7, INPUT_PULLUP);

  //Green Button
  pinMode(A2, OUTPUT);
  pinMode(0, INPUT_PULLUP);

  //Lever Switch
  pinMode(8, INPUT_PULLUP);
}

void success(void){
  //Make sure to turn off all buttons
  digitalWrite(1, LOW);
  digitalWrite(A0, LOW);
  digitalWrite(A1, LOW);
  digitalWrite(A2, LOW);

  //First flash sequence (Blue -> Yellow -> Red -> Green)
  digitalWrite(1, HIGH);
  tone(11, success_tone[0], success_duration[0]);
  delay(50);
  digitalWrite(1, LOW);
  digitalWrite(A0, HIGH);
  delay(50);
  digitalWrite(A0, LOW);
  digitalWrite(A1, HIGH);
  delay(50);
  noTone(11);
  tone(11, success_tone[0], success_duration[0]);
  digitalWrite(A1, LOW);
  digitalWrite(A2, HIGH);
  delay(50);
  digitalWrite(A2, LOW);


  //Second flash sequence (Red -> Yellow -> Green -> Blue)
  digitalWrite(A1, HIGH);
  delay(50);
  digitalWrite(A1, LOW);
  digitalWrite(A0, HIGH);
  delay(50);
  noTone(11);
  tone(11, success_tone[0], success_duration[0]);
  digitalWrite(A0, LOW);
  digitalWrite(A2, HIGH);
  delay(50);
  digitalWrite(A2, LOW);
  digitalWrite(1, HIGH);
  delay(50);
  digitalWrite(1, LOW);

  //Third flash sequence (Yellow -> Green -> Blue -> Red)
  digitalWrite(A0, HIGH);
  delay(50);
  noTone(11);
  tone(11, success_tone[0], success_duration[0]);
  digitalWrite(A0, LOW);
  digitalWrite(A2, HIGH);
  delay(50);
  digitalWrite(A2, LOW);
  digitalWrite(1, HIGH);
  delay(50);
  digitalWrite(1, LOW);
  digitalWrite(A1, HIGH);
  delay(50);
  noTone(11);
  digitalWrite(A1, LOW);
}

boolean playing(void)
{
  return TIMSK1 & _BV(OCIE1A);
}

int buttonstatus(){
  if (digitalRead(6) == LOW) return 1;
  if (digitalRead(5) == LOW) return 2;
  if (digitalRead(7) == LOW) return 3;
  if (digitalRead(0) == LOW) return 4;
  return -1;
}

int sliderthreshold(int current_threshold){
  int current_value = analogRead(A3);
  int next_threshold = current_threshold;
  if (current_value < current_threshold && current_value > current_threshold - 50) next_threshold = current_value - 50;
  if (next_threshold <= 300) next_threshold = -1;
  return next_threshold;
}

void loop() {
  bool game_end = false; //This var controls end of game logic

  randomSeed(millis()); //Set seed each time

  int number = random(1, 3); //Pick random int between 1 and 3
  int action = -1;

  if (number == 1){
    action = random(1, 5); //Pick random int from 1 to 4
  }
  if (number == 2){
    action = 5;
  }
  if (number == 3){
    action = 6;
  }
  
  if (action == 1 || action == 2 || action == 3 || action == 4){
    display.clearDisplay();
    display.display();
    startPlayback(push, sizeof(push));
    while (playing());
    display.clearDisplay();
    display.setCursor(0, 0);
    display.println(F("Push It!"));
    display.display();
    if (action==1) digitalWrite(1, HIGH);
    if (action==2) digitalWrite(A0, HIGH);
    if (action==3) digitalWrite(A1, HIGH);
    if (action==4) digitalWrite(A2, HIGH);
  }
 
  if (action == 5){
    display.clearDisplay();
    display.display();
    startPlayback(reopen, sizeof(reopen));
    while (playing());
    display.clearDisplay();
    display.setCursor(0, 0);
    display.println(F("Reopen It!"));
    display.display();     
  }

  if (action == 6){
    display.clearDisplay();
    display.setCursor(0, 0);
    display.println(F("Slide It!"));
    display.display();     
  }

  int button = -1;
  int condition = 1;
  int starttime = millis();
  bool lever = false;
  int leverswitch = 0;
  bool nextlever = false;
  int threshold = 650;

  //Start Background Music
  int last_tone_start = millis();
  int current_tone = 0;
  tone(11, background_tone[0], background_duration[0]);
  
  while (condition == 1){
    if (millis() - starttime > threshold) condition = -1;

    //Check if we need to advance to the next tone of the background music
    if (millis() - last_tone_start > background_duration[current_tone]){
      last_tone_start = millis();
      if (current_tone < 6) current_tone++;
      else current_tone = 0;
      if (background_tone[current_tone] > 0) tone(11, background_tone[current_tone], background_duration[current_tone]);
      else noTone(11);
    }
    button = buttonstatus();
    threshold = sliderthreshold(threshold);
    nextlever = (digitalRead(8) == LOW);
    
    if (nextlever != lever) leverswitch++;
    else leverswitch = 0;

    if (button != -1){
      if (button == action) condition = 0;
      else condition = -1;
    }
    else if (lever == true && nextlever == false && leverswitch == 30){
			if (action == 5) condition = 0;
			else condition = -1;
		}
    else if (threshold == -1){
			if (action == 6) condition = 0;
			else condition = -1;
    }

    if (leverswitch == 30){
      lever = nextlever;
      leverswitch = 0;
      starttime = millis();      
    }
  }

  noTone(11);
  int response_start = millis();

  if (action > 0 || action < 4){
    digitalWrite(1, LOW);
    digitalWrite(A0, LOW);
    digitalWrite(A1, LOW);
    digitalWrite(A2, LOW);
  }

  if (condition == 0 && score < 10){
    display.clearDisplay();
    display.setCursor(0, 0);
    display.println(F("Success!"));
    display.display();
    success();
    score++;
  }
  else if (condition == 0){
    display.clearDisplay();
    display.setCursor(0, 0);
    display.println(F("You Win!!!"));
    display.display();

    digitalWrite(1, HIGH);
    digitalWrite(A0, HIGH);
    digitalWrite(A1, HIGH);
    digitalWrite(A2, HIGH);

    //Play victory sound
    last_tone_start = millis();
    current_tone = 0;
    while (current_tone != -1){
      if (millis() - last_tone_start > victory_duration[current_tone]){
        last_tone_start = millis();
        if (current_tone < 7){
          current_tone++;
          if (victory_tone[current_tone] > 0) tone(11, victory_tone[current_tone], victory_duration[current_tone]);
          else noTone(11);
        }
        else current_tone == -1;
      }
    }
    noTone(11);
    game_end = true;
  }
  
  if (condition == -1){
    display.clearDisplay();
    display.setCursor(0, 0);
    display.println(F("You Lose..."));
    display.display();

    //Play defeat sound
    last_tone_start = millis();
    current_tone = 0;
    while (current_tone != -1){
      if (millis() - last_tone_start > defeat_duration[current_tone]){
        last_tone_start = millis();
        if (current_tone < 3){
          current_tone++;
          if (defeat_tone[current_tone] > 0) tone(11, defeat_tone[current_tone], defeat_duration[current_tone]);
          else noTone(11);
        }
        else current_tone == -1;
      }
    }
    noTone(11);
    game_end = true;
  }

  if (game_end == false){
    threshold = threshold * 0.9;
  	if (threshold < 650) threshold = 650;
    while (millis() - response_start < threshold);
  }
  else{
    delay(2000);
    display.clearDisplay();
    display.setCursor(0, 0);
    display.println(F("To restart the game, please press and hold all four buttons until a beep is heard."));
    display.display();

    digitalWrite(1, HIGH);
    digitalWrite(A0, HIGH);
    digitalWrite(A1, HIGH);
    digitalWrite(A2, HIGH);
    
    int breakout_streak = 0;
    while (breakout_streak < 33){
      if (digitalRead(6) == LOW && digitalRead(5) == LOW && digitalRead(7) == LOW && digitalRead(0) == LOW) breakout_streak++;
      else breakout_streak = 0;
      delay(10);
    }

    tone(11, 400, 100);
    delay(1000);
    noTone(11);
  }
}